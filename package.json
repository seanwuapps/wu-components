{
  "name": "wu-components",
  "version": "0.2.23",
  "description": "Web Up Components",
  "module": "dist/esm/es5/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "release:patch": "npm run build ; npm version patch ; npm publish",
    "release:minor": "npm run build ; npm version minor ; npm publish",
    "release:major": "npm run build ; npm version major ; npm publish",
    "docs.data": "stencil build --docs-json ./docs.json"
  },
  "dependencies": {
    "rellax": "^1.9.1"
  },
  "devDependencies": {
    "@stencil/core": "^0.18.1",
    "@stencil/postcss": "^0.1.0",
    "@stencil/sass": "^0.2.1",
    "@stencil/utils": "0.0.5",
    "@types/autoprefixer": "^6.7.3",
    "@types/jest": "^23.3.14",
    "autoprefixer": "^9.5.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "postcss-normalize": "^7.0.1",
    "postcss-pxtorem": "^4.0.1",
    "postcss-scss": "^2.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-strict-scss": "^0.5.0",
    "stylelint-scss": "^3.6.1",
    "workbox-build": "3.4.1"
  },
  "author": "Sean Wu",
  "license": "MIT",
  "homepage": "https://seanwuapps.github.io/wu-components",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
